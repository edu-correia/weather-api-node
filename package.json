{
  "name": "weather-api-node",
  "version": "1.0.0",
  "description": "Backend responsável por prover dados sobre as condições climáticas em praias",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:unit": "jest",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardo-ehsc/weather-api-node.git"
  },
  "keywords": [],
  "author": "Eduardo Correia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eduardo-ehsc/weather-api-node/issues"
  },
  "homepage": "https://github.com/eduardo-ehsc/weather-api-node#readme",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.11.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  }
}
